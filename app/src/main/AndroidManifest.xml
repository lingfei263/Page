<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.ffb.page.app">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppXTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimplePageActivity1" />
        <activity android:name=".SimplePageActivity2" />
        <activity android:name=".NetworkPageActivity" />
        <activity android:name=".SwipeRefreshLayoutPageActivity" />
        <activity android:name=".OkHttpPageActivity" />
        <activity android:name=".AutoRefreshPageActivity" />
        <activity android:name=".RecyclerPageActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".CheckedPageActivity" />
    </application>

</manifest>